<h1>Ele Swims</h1>

<div>
	<% @blogs.each do |blog|%>
		<p><%= link_to blog.title, blog_path(blog) %></p>
		<%= image_tag blog.image.url(:thumb) %>
		<p><%= blog.body %></p>
	<% end %>
</div>
<% if current_user && current_user.administrator %>
<p><%= link_to "Add Blog", new_blog_path %></p>
<% end %>